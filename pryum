# END of included PKGS ----

fedpkg="https://download.fedoraproject.org/"

if [ -f /etc/os-release ]; then
    # freedesktop.org and systemd
    . /etc/os-release
fi

for i in /etc/pryum.conf /pryum.conf; do
  if [ -f $i ]; then
    source $i
    break
  fi
done

rundnf=false
if [ "x$ID" = "xfedora" ]; then
    rundnf=true
    echo "Using Fedora $VERSION_ID"
    case "x$VERSION_ID"  in
    "x32")
        PKGS=("${PKGS32[@]}")
    ;;
    "x31")
        PKGS=("${PKGS31[@]}")
    ;;
    "x30")
        PKGS=("${PKGS30[@]}")
    ;;
    "x29")
        PKGS=("${PKGS29[@]}")
    ;;
    "x28")
        PKGS=("${PKGS28[@]}")
    ;;
    *)
        echo "Unknown Fedora, using default Fedora 31"
        rundnf=false
        VERSION_ID=31
        PKGS=("${PKGS31[@]}")
    esac
else
    VERSION_ID=31
    echo "Using default Fedora 31"
    PKGS=("${PKGS31[@]}")
fi

function cmdinstall {
# mhost="$(curl -s --head $fedpkg | fgrep location | | awk '{ print $2 }')"
mhost="$(curl -s --head $fedpkg | sed -ne 's/location: \(.*\)$/\1/p')"

# echo $mhost

for pkg in "${PKGS[@]}" ; do
    NAME=${pkg%%:*}
    URLs=${pkg#*:}
    BNAME=$(basename $NAME)
    if [ "x$1" != "x$NAME" -a "x$1" != "x$BNAME" ]; then
        continue
    fi

    tmpfile=$(mktemp /tmp/pryum.XXXXXX)
    trap 'rm -f -- "$tmpfile"' INT TERM HUP EXIT

    echo -n "dnf --disablerepo='*' in" > $tmpfile
    for URL in $URLs ; do
        repoURL=${URL%%/*}
        pkgURL=${URL#*/}
        case "$repoURL" in
            fedora | f)
                off="releases/$VERSION_ID/Everything/x86_64/os/Packages"
            ;;
            fedora-modular | f-m)
                off="releases/$VERSION_ID/Modular/x86_64/os/Packages"
            ;;
            rawhide | r)
                off="development/rawhide/Everything/x86_64/os/Packages"
            ;;
            *)
                echo "Unknown repo."
                exit 4
            ;;
        esac
        echo " \\" >> $tmpfile
        echo -n "$mhost$off/$pkgURL" >> $tmpfile
    done
    echo '' >> $tmpfile
    if $rundnf; then
        echo "Running dnf locally for $NAME:"
        . $tmpfile
    else
        cat $tmpfile
    fi

    break
done
# echo -e "${PKGS[1]%%:*} => ${PKGS[1]#*:}\n"
}

function cmdlist {
IFS=$'\n'
SPKGS=($(sort <<<"${PKGS[*]}"))
IFS=' '
maxlen=14
for pkg in "${SPKGS[@]}" ; do
    NAME=${pkg%%:*}
    URLs=${pkg#*:}
    BNAME=$(basename $NAME)

    # No args. show everything ... some args. show them.
    if [ "x$1" != "x" ]; then
        found=false
        for i in "$@" ; do
            if [ "x$i" = "x$NAME" -o "x$i" = "x$BNAME" ]; then
                found=true
                break
            fi
        done
        if ! $found; then
            continue
        fi
    fi

    URL=x
    count=0
    for v in $URLs ; do
        count=$(($count + 1))
        if [ "x$count" = "x1" ]; then
            URL=$(basename $v)
        fi
    done
    if [ "x$count" != "x1" ]; then
    if [ "x$count" != "x2" ]; then
    if [ "x$count" != "x0" ]; then
        printf "%*s => %s (+%d deps)\n"  $maxlen "$NAME" "$URL" $(($count - 1))
    else
        printf "%*s => Not available.\n" $maxlen "$NAME"
    fi
    else
        printf "%*s => %s (+%d dep)\n"   $maxlen "$NAME" "$URL" $(($count - 1))
    fi
    else
        printf "%*s => %s\n"             $maxlen "$NAME" "$URL"
    fi
done
}

function cmdsearch {
IFS=$'\n'
SPKGS=($(sort <<<"${PKGS[*]}"))
IFS=' '
maxlen=14
for pkg in "${SPKGS[@]}" ; do
    NAME=${pkg%%:*}
    URLs=${pkg#*:}
    BNAME=$(basename $NAME)

    # No args. show everything ... some args. show them.
    if [ "x$1" != "x" ]; then
        found=false
        for i in "$@" ; do
            case "$NAME" in
            *"$i"*)
                found=true
                break
            ;;
            esac
        done
        if ! $found; then
            continue
        fi
    fi

    URL=x
    count=0
    for v in $URLs ; do
        count=$(($count + 1))
        if [ "x$count" = "x1" ]; then
            URL=$(basename $v)
        fi
    done
    if [ "x$count" != "x1" ]; then
    if [ "x$count" != "x2" ]; then
    if [ "x$count" != "x0" ]; then
        printf "%*s => %s (+%d deps)\n"  $maxlen "$NAME" "$URL" $(($count - 1))
    else
        printf "%*s => Not available.\n" $maxlen "$NAME"
    fi
    else
        printf "%*s => %s (+%d dep)\n"   $maxlen "$NAME" "$URL" $(($count - 1))
    fi
    else
        printf "%*s => %s\n"             $maxlen "$NAME" "$URL"
    fi
done
}

function cmdinfo {
IFS=$'\n'
SPKGS=($(sort <<<"${PKGS[*]}"))
IFS=' '
for pkg in "${SPKGS[@]}" ; do
    NAME=${pkg%%:*}
    URLs=${pkg#*:}
    BNAME=$(basename $NAME)

    # No args. show everything ... some args. show them.
    if [ "x$1" != "x" ]; then
        found=false
        for i in "$@" ; do
            if [ "x$i" = "x$NAME" -o "x$i" = "x$BNAME" ]; then
                found=true
                break
            fi
        done
        if ! $found; then
            continue
        fi
    fi

    count=0
    for v in $URLs ; do
        count=$(($count + 1))
        if [ "x$count" = "x1" ]; then
            URL=$(basename $v)
        fi
    done
    echo "Name: $NAME"
    echo "Pkg(s): $count"
    count=0
    for v in $URLs ; do
        count=$(($count + 1))
        printf "  %2d. " $count
        echo $(basename $v)
    done
done
}

cmd="$1"
if [ "x$1" = "x" ]; then
 cmd=ls
fi


case "$cmd" in
 show-install | show-in | showinstall | showin)
    rundnf=false
    cmd=install
 ;;
esac

case "$cmd" in
 install | in)
    if ! $dnfrun; then
        echo "Run:"
    fi
    while [ "x$2" != "x" ]; do
        cmdinstall $2
        shift
    done
 ;;

 list | ls)
    shift
    cmdlist $@
  ;;

 info)
    shift
    cmdinfo $@
  ;;

 search | srch | sr)
    shift
    cmdsearch $@
  ;;

 redeps)
    PATH="$PATH:/mnt:."
for pkg in "${PKGS[@]}" ; do
    NAME=${pkg%%:*}
    dnf-resolve.py -q $NAME || true
done
  ;;

esac

exit 0




